package com.basicsquestions;

public class LCMNumber {
	public static int lcm(int a,int b)
	{
		int res=a>b?a:b;
		while(true)
		{
			if(res%a==0&&res%b==0)
			{
				break;
			}
			res++;
		}
		return res;
	}
	public static int gcd(int a,int b)
	{
		int res=1;
		while(true)
		{
			if(a%res==0&&b%res==0)
			{
				break;
			}
			res++;
		}
		return res;
	}
	public static void main(String[] args) {
		System.out.println(gcd(6,8));
		
	}

}

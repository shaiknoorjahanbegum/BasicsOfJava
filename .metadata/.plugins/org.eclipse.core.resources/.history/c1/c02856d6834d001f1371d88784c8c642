package com.basicsOOPs;

//Example 1
class MyThread extends Thread
{
	public void run()
	{
		for(int i=1;i<=10;i++)
		{
			System.out.println(i);
			try {
				Thread.sleep(3424);
			} catch (InterruptedException e) {
			 System.out.println("....");
			}
		}
	}
}

public class MultiThreadingExample {
}

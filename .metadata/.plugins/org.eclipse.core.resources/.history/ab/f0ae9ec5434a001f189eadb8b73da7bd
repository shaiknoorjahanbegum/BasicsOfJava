package com.basicsquestions;

public class FactorialOfNumber {
	public static int fact(int n)
	{
		int fact=1;
		for(int i=1;i<=n;i++)
		{
			fact*=i;
		}
		return fact;
	}
	//in range
	public static void factRange(int n,int m)
	{
		int fact=1;
		for(int i=n;i<=m;i++)
		{
			fact=1;
			for(int j=1;j<=i;j++)
			{
				fact*=j;
			}
			System.out.println(fact);
		}
	}
	public static void main(String[] args) {
		factRange(1,10);
	}

}
